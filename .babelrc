{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    ["transform-class-properties", { "spec": false }]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
